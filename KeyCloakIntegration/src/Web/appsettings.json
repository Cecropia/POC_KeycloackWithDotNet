{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=KeyCloakIntegrationDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Authentication": {
    "Keycloak": {
      "Authority": "http://keycloak:8080/realms/your-realm",
      "ClientId": "your-client-id",
      "ClientSecret": "your-client-secret",
      "Audience": "your-api-audience"
    }
  },
  "AllowedHosts": "*"
}
